   "source": [
    "# Determinaçao da indutancia, em Henrys\n",
    "from sympy import *\n",
    "init_printing(use_unicode=True)\n",
    "g1, g2, mu0, A1, A2, i, N = symbols('g1 g2 mu0 A1 A2 i N')\n",
    "R1 = g1/(mu0*A1)\n",
    "R2 = g2/(mu0*A2)\n",
    "phi = (N*i)/(R1*R2/(R1 + R2))\n",
    "lambd = N*phi\n",
    "L = lambd/i\n",
    "L"
   ]
  },
