   "source": [
    "# Desenho do circuito:\n",
    "import SchemDraw as schem\n",
    "import SchemDraw.elements as e\n",
    "#  :-)\n",
    "#import matplotlib.pyplot as plt\n",
    "#plt.xkcd()\n",
    "d = schem.Drawing()\n",
    "F1 = d.add(e.SOURCE_V, d='up', label='Ni')\n",
    "L1 = d.add(e.LINE, d='right')\n",
    "d.add(e.DOT)\n",
    "d.push()\n",
    "R1 = d.add(e.RES, d='down', label='$\\mathcal{R}_1$')\n",
    "d.add(e.DOT)\n",
    "d.pop()\n",
    "L2 = d.add(e.LINE)\n",
    "R2 = d.add(e.RES, d='down', label='$\\mathcal{R}_2$')\n",
    "L3 = d.add(e.LINE, d='left', tox=F1.start)\n",
    "d.draw()\n",
    "d.save('ex1_3.pdf')"
   ]
